<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="PreferenceHeader">
        <attr name="id" format="integer" />
        <attr name="title" />
        <attr name="summary" format="string" />
        <attr name="breadCrumbTitle" format="string" />
        <attr name="breadCrumbShortTitle" format="string" />
        <attr name="icon" />
        <attr name="fragment" format="string" />
        <attr name="help" format="reference" />
    </declare-styleable>
    <declare-styleable name="ScrollLayout">
        <attr name="labelerClass" format="string" />
        <attr name="labelerFormat" format="string" />
        <attr name="childWidth" format="dimension" />
        <attr name="childHeight" format="dimension" />
    </declare-styleable>
    <declare-styleable name="ImageMultiChoiceListPreference">
        <attr name="entryImages" format="reference" />
    </declare-styleable>

    <attr name="activatedBackgroundIndicator" format="reference" />

</resources>