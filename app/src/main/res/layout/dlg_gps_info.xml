<?xml version="1.0" encoding="utf-8"?>
<TableLayout xmlns:android="http://schemas.android.com/apk/res/android"
    style="@style/GeneralDialog"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:stretchColumns="0,1" >

    <TableRow>

        <TextView
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:text="@string/satellites" />

        <TextView
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:text="@string/lastfix" />
    </TableRow>

    <TableRow>

        <TextView
            android:id="@+id/sats"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:text="--/--"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/lastfix"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:text="--"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:textStyle="bold" />
    </TableRow>

    <TableRow android:paddingTop="5dip" >

        <TextView
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:text="@string/hdop" />

        <TextView
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:text="@string/vdop" />
    </TableRow>

    <TableRow>

        <TextView
            android:id="@+id/hdop"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:text="--"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/vdop"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:text="--"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:textStyle="bold" />
    </TableRow>

    <TableRow android:paddingTop="5dip" >

        <TextView
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:text="@string/accuracy" />

        <TextView
            android:layout_width="fill_parent"
            android:layout_height="wrap_content" />
    </TableRow>

    <TableRow>

        <TextView
            android:id="@+id/accuracy"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:text="--"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/nothing"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:textStyle="bold" />
    </TableRow>

</TableLayout>